{
  "expo": {
    "jsEngine": "hermes",
    "name": "capture-your-diary-in-calendar",
    "slug": "capture-your-diary-in-calendar",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yuhyeon.captureyourdiaryincalendar",
      "jsEngine": "jsc"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.yuhyeon.captureyourdiaryincalendar"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-image-picker",
        {
          "photosPermission": "캘린더에 사진을 업로드하기 위해 사진 접근 권한이 필요합니다."
        }
      ],
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  },
  "react-native-google-mobile-ads": {
    "android_app_id": "ca-app-pub-5956225840587687~2121397424",
    "ios_app_id": "ca-app-pub-5956225840587687~3934894740"
  }
}
